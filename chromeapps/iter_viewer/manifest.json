{
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "name": "Chromium Iteration Viewer",
  "version": "4.0",
  "description": "Shows the current Chromium Iteration when looking at the issue tracker.",
  "icons": {
    "128": "icon-128x128.png"
  },
  "page_action": {
    "default_title": "Current Chromium Iteration"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "alarms",
    "storage",
    "tabs",
    "webNavigation",
    "http://chromepmo.appspot.com/schedule/iteration/json"
  ]
}
